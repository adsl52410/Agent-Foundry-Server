{
    "openapi": "3.0.0",
    "info": {
        "title": "Plugin Registry API",
        "description": "Agent-Foundry Plugin Registry Server API 文檔",
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "http://localhost:8089/api/v1",
            "description": "本地開發伺服器"
        }
    ],
    "paths": {
        "/plugins": {
            "get": {
                "tags": [
                    "Plugins"
                ],
                "summary": "List all plugins",
                "description": "Get a paginated list of all available plugins with optional filtering",
                "operationId": "c20e948a14c9c8cb7a911c314bbed71c",
                "parameters": [
                    {
                        "name": "search",
                        "in": "query",
                        "description": "Search by plugin name or description",
                        "required": false,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "author",
                        "in": "query",
                        "description": "Filter by author",
                        "required": false,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "page",
                        "in": "query",
                        "description": "Page number for pagination",
                        "required": false,
                        "schema": {
                            "type": "integer",
                            "default": 1
                        }
                    },
                    {
                        "name": "per_page",
                        "in": "query",
                        "description": "Items per page (max: 100)",
                        "required": false,
                        "schema": {
                            "type": "integer",
                            "default": 20,
                            "maximum": 100
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Success",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "data": {
                                            "type": "array",
                                            "items": {
                                                "type": "object"
                                            }
                                        },
                                        "meta": {
                                            "type": "object"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            },
            "post": {
                "tags": [
                    "Plugins"
                ],
                "summary": "Upload/Publish plugin",
                "description": "Upload a new plugin or new version of existing plugin",
                "operationId": "495021eb9b1b62bac5b6f14cbbe9fc09",
                "requestBody": {
                    "required": true,
                    "content": {
                        "multipart/form-data": {
                            "schema": {
                                "properties": {
                                    "plugin_file": {
                                        "description": "The plugin.py file",
                                        "type": "string",
                                        "format": "binary"
                                    },
                                    "manifest_file": {
                                        "description": "The manifest.json file (optional)",
                                        "type": "string",
                                        "format": "binary"
                                    },
                                    "additional_files": {
                                        "description": "Additional files",
                                        "type": "array",
                                        "items": {
                                            "type": "string",
                                            "format": "binary"
                                        }
                                    },
                                    "name": {
                                        "description": "Plugin name (required if manifest not provided)",
                                        "type": "string"
                                    },
                                    "version": {
                                        "description": "Version override (defaults to manifest version)",
                                        "type": "string"
                                    },
                                    "description": {
                                        "description": "Description override",
                                        "type": "string"
                                    },
                                    "author": {
                                        "description": "Author override",
                                        "type": "string"
                                    }
                                },
                                "type": "object"
                            }
                        }
                    }
                },
                "responses": {
                    "201": {
                        "description": "Plugin published successfully"
                    },
                    "422": {
                        "description": "Validation error"
                    },
                    "401": {
                        "description": "Unauthorized"
                    }
                },
                "security": [
                    {
                        "bearerAuth": []
                    }
                ]
            }
        },
        "/plugins/{name}": {
            "get": {
                "tags": [
                    "Plugins"
                ],
                "summary": "Get plugin details",
                "description": "Get detailed information about a specific plugin including all versions",
                "operationId": "e825545c163ae80d7d716d5825ccef84",
                "parameters": [
                    {
                        "name": "name",
                        "in": "path",
                        "description": "Plugin name",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Success"
                    },
                    "404": {
                        "description": "Plugin not found"
                    }
                }
            }
        },
        "/plugins/{name}/versions/{version}": {
            "get": {
                "tags": [
                    "Plugins"
                ],
                "summary": "Get plugin version details",
                "description": "Get detailed information about a specific plugin version",
                "operationId": "558792be2ffac78495c2671b8ef583ed",
                "parameters": [
                    {
                        "name": "name",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "version",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Success"
                    },
                    "404": {
                        "description": "Plugin version not found"
                    }
                }
            }
        },
        "/plugins/{name}/download": {
            "get": {
                "tags": [
                    "Plugins"
                ],
                "summary": "Download latest version",
                "description": "Download the latest version of a plugin",
                "operationId": "9cb9ed8292bafe7493e60d15160b8c61",
                "parameters": [
                    {
                        "name": "name",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "format",
                        "in": "query",
                        "description": "Response format: zip or json",
                        "required": false,
                        "schema": {
                            "type": "string",
                            "default": "zip",
                            "enum": [
                                "zip",
                                "json"
                            ]
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Success"
                    },
                    "404": {
                        "description": "Plugin not found"
                    }
                }
            }
        },
        "/plugins/{name}/versions/{version}/download": {
            "get": {
                "tags": [
                    "Plugins"
                ],
                "summary": "Download plugin version",
                "description": "Download plugin files as a ZIP archive or JSON with file URLs",
                "operationId": "c0180ef87faef7b1c1e0133095974c0b",
                "parameters": [
                    {
                        "name": "name",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "version",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "format",
                        "in": "query",
                        "description": "Response format: zip or json",
                        "required": false,
                        "schema": {
                            "type": "string",
                            "default": "zip",
                            "enum": [
                                "zip",
                                "json"
                            ]
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Success"
                    },
                    "404": {
                        "description": "Plugin version not found"
                    }
                }
            }
        },
        "/plugins/{name}/versions/{version}/files/{filename}": {
            "get": {
                "tags": [
                    "Plugins"
                ],
                "summary": "Download specific file",
                "description": "Download a specific file from a plugin version",
                "operationId": "1bed48543a6eb86030d5e253271a60dc",
                "parameters": [
                    {
                        "name": "name",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "version",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "filename",
                        "in": "path",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Success"
                    },
                    "404": {
                        "description": "File not found"
                    }
                }
            }
        },
        "/plugins/search": {
            "get": {
                "tags": [
                    "Plugins"
                ],
                "summary": "Search plugins",
                "description": "Advanced search with multiple criteria",
                "operationId": "d6d26249e8ffdc29eaaf22fa0fa1f935",
                "parameters": [
                    {
                        "name": "q",
                        "in": "query",
                        "description": "Search query",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "name": "field",
                        "in": "query",
                        "description": "Search field: name, description, author, or all",
                        "required": false,
                        "schema": {
                            "type": "string",
                            "default": "all",
                            "enum": [
                                "name",
                                "description",
                                "author",
                                "all"
                            ]
                        }
                    },
                    {
                        "name": "page",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "type": "integer",
                            "default": 1
                        }
                    },
                    {
                        "name": "per_page",
                        "in": "query",
                        "required": false,
                        "schema": {
                            "type": "integer",
                            "default": 20,
                            "maximum": 100
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Success"
                    }
                }
            }
        },
        "/plugins/index": {
            "get": {
                "tags": [
                    "Plugins"
                ],
                "summary": "Get plugin index",
                "description": "Get a simplified index similar to index.json format",
                "operationId": "693d8518b894374a11ca6f43e0052dd8",
                "responses": {
                    "200": {
                        "description": "Success"
                    }
                }
            }
        }
    },
    "components": {
        "securitySchemes": {
            "bearerAuth": {
                "type": "http",
                "scheme": "bearer",
                "bearerFormat": "JWT",
                "description": "Enter JWT token in format (Bearer <token>)"
            }
        }
    },
    "tags": [
        {
            "name": "Plugins",
            "description": "Plugins"
        }
    ]
}